<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>MESSAGES</key>
	<string>Mesaje</string>

	<!-- Switch in Messages app to enable/disable typing notifications. -->
	<key>SEND_TYPING_NOTIFICATIONS</key>
	<string>Trimite Notificari De Tastare</string>

	<!-- Explanation of the typing switch. -->
	<key>DISABLE_TYPING_NOTIFICATIONS_EXPLANATION</key>
	<string>Permite sau restrictioneaza notificarea de tastare (trimisa persoanei care primeste mesajul).</string>

	<!-- Switch in Messages app to enable/disable read receipt notifications. -->
	<key>SEND_READ_RECEIPTS</key>
	<string>Trimite Confirmari Citire</string>

	<!-- Explanation of the read receipt switch. -->
	<key>DISABLE_READ_RECEIPTS_EXPLANATION</key>
	<string>Permite sau restrictioneaza notificarea de mesaj citit (trimisa persoanei care primeste mesajul).</string>

	<!-- Heading for the list of people. -->
	<key>PEOPLE</key>
	<string>Persoane</string>

	<!-- Button that allows another person to be added to the list. -->
	<key>ADD_PERSON</key>
	<string>Adăugați…</string>

	<!-- Button that allows a person to be removed. -->
	<key>REMOVE</key>
	<string>Elimină</string>
</dict>
</plist>
