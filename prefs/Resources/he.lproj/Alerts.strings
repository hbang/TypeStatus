<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>התרעות</string>

	<!-- Header for the animation type segmented control. -->
	<key>ANIMATION_TYPE</key>
	<string>אנימציה</string>

	<!-- Buttons shown in the segmented control for the animation type. -->
	<key>ANIMATION_SLIDE</key>
	<string>החלקה</string>
	<key>ANIMATION_FADE</key>
	<string>דהייה</string>

	<!-- Header for the display duration selection. -->
	<key>DISPLAY_DURATION</key>
	<string>זמן תצוגה</string>

	<!-- The label showing how many seconds the user has specified. -->
	<key>1_SECOND</key>
	<string>שנייה 1</string>
	<key>N_SECONDS</key>
	<string>%i שניות</string>

	<!-- The Typing and Read Receipt headings. -->
	<key>TYPING_HEADER</key>
	<string>מקליד/ה</string>
	<key>READ_HEADER</key>
	<string>אישור קריאה</string>

	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>כבוי</string>
	<key>ALERT_TYPE_ICON</key>
	<string>אייקון בלבד</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>סטטוס בר</string>

	<!-- Button for choosing how long the alert shows for. -->
	<key>SHOW_FOR</key>
	<string>הצג למשך</string>

	<!-- Options for "Show For". -->
	<key>SHOW_FOR_SPECIFIED</key>
	<string>הזמן המוגדר שלי</string>
	<key>SHOW_FOR_TYPING</key>
	<string>משך ההקלדה</string>

	<!-- Explanation shown under the "Show For" list. -->
	<key>SHOW_FOR_EXPLANATION</key>
	<string>כדי להציג את ההתראה למשך המוגדר בדף הקודם, בחר/י ״הזמן המוגדר שלי״. כדי להראות את ההתראה לכל משך הזמן בו השולח/ת מקליד/ה, בחר/י ״משך ההקלדה״.</string>

	<!-- Switch to allow or disallow alerts showing in Messages. -->
	<key>HIDE_IN_MESSAGES</key>
	<string>הסתר בהודעות</string>

	<!-- Buttons to test the alerts. -->
	<key>TEST_TYPING</key>
	<string>בדיקת התראת הקלדה</string>
	<key>TEST_READ_RECEIPT</key>
	<string>בדיקת התראת אישור קריאה</string>
</dict>
</plist>
