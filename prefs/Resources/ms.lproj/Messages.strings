<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>MESSAGES</key>
	<string>Mesej</string>

	<!-- Switch in Messages app to enable/disable typing notifications. -->
	<key>SEND_TYPING_NOTIFICATIONS</key>
	<string>Hantar Makluman Menulis</string>

	<!-- Explanation of the typing switch. -->
	<key>DISABLE_TYPING_NOTIFICATIONS_EXPLANATION</key>
	<string>Membenarkan atau menghalang penerima itu daripada melihat ketika anda sedang menulis.</string>

	<!-- Switch in Messages app to enable/disable read receipt notifications. -->
	<key>SEND_READ_RECEIPTS</key>
	<string>Hantar Penerimaan Bacaan</string>

	<!-- Explanation of the read receipt switch. -->
	<key>DISABLE_READ_RECEIPTS_EXPLANATION</key>
	<string>Membenarkan atau menghalang penerima itu daripada melihat ketika anda membaca mesej mereka.</string>

	<!-- Button that allows a person to be removed. -->
	<key>REMOVE</key>
	<string>Alih Keluar</string>
</dict>
</plist>
