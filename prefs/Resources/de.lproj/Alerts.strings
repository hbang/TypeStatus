<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>Benachrichtigungen</string>

	<!-- Button for choosing the alert format. -->
	<key>OVERLAY_FORMAT</key>
	<string>Benachrichtigungsformat</string>

	<!-- Options for the alert format. -->
	<key>OVERLAY_FORMAT_NATURAL</key>
	<string>Natürlich</string>
	<key>OVERLAY_FORMAT_NATURAL_WITH_EXAMPLE</key>
	<string>Natürlich: Johnny Appleseed schreibt</string>
	<key>OVERLAY_FORMAT_TRADITIONAL</key>
	<string>Traditionell</string>
	<key>OVERLAY_FORMAT_TRADITIONAL_WITH_EXAMPLE</key>
	<string>Traditionell: Tippt: Johnny Appleseed</string>
	<string>Nur Name</string>
	<key>OVERLAY_FORMAT_NAME_ONLY_WITH_EXAMPLE</key>
	<string>Nur Name: Johnny Appleseed</string>
 
	<!-- Header for the animation type segmented control. -->
	<key>ANIMATION_TYPE</key>
	<string>Animation</string>
 
	<!-- Buttons shown in the segmented control for the animation type. -->
	<key>ANIMATION_SLIDE</key>
	<string>Gleiten</string>
	<key>ANIMATION_FADE</key>
	<string>Ausblenden</string>
 
	<!-- Header for the display duration selection. -->
	<key>DISPLAY_DURATION</key>
	<string>Anzeigedauer</string>
 
	<!-- The label showing how many seconds the user has specified. -->
	<key>ONE_SECOND</key>
	<string>1 Sekunde</string>
	<key>N_SECONDS</key>
	<string>%i Sekunden</string>
 
	<!-- Header for the section shown when libstatusbar isn't installed. -->
	<key>LIBSTATUSBAR_NOT_INSTALLED</key>
	<string>libstatusbar wird für Symbole in der Statusleiste benötigt.</string>

	<!-- Button that opens Cydia to the libstatusbar package. -->
	<key>INSTALL_LIBSTATUSBAR</key>
	<string>libstatusbar installieren</string>

	<!-- The Typing and Read Receipt headings. -->
	<key>TYPING_HEADER</key>
	<string>Tippt</string>
	<key>READ_HEADER</key>
	<string>Gelesen</string>
 
	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Aus</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Nur Symbol</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Statusleiste</string>
 
	<!-- Button for choosing how long the alert shows for. -->
	<key>SHOW_FOR</key>
	<string>Zeigen für</string>
 
	<!-- Options for "Show For". -->
	<key>SHOW_FOR_SPECIFIED</key>
	<string>Eigene Anzeigedauer</string>
	<key>SHOW_FOR_TYPING</key>
	<string>Schreibdauer</string>
 
	<!-- Explanation shown under the "Show For" list. -->
	<key>SHOW_FOR_EXPLANATION</key>
	<string>Um die Benachrichtigung für eine gewählte Zeit anzuzeigen, aktiviere „Eigene Anzeigedauer“. „Schreibdauer“ blendet die Benachrichtigung ein, bis der Absender das Tippen beendet".</string>
 
	<!-- Switch to allow or disallow alerts showing in Messages. -->
	<key>HIDE_IN_MESSAGES</key>
	<string>In Nachrichten-App ausblenden</string>
 
	<!-- Buttons to test the alerts. -->
	<key>TEST_TYPING</key>
	<string>Teste Tippen Benachrichtigung</string>
	<key>TEST_READ_RECEIPT</key>
	<string>Teste Lesebestätigung</string>
