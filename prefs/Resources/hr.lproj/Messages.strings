<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>MESSAGES</key>
	<string>Poruke</string>

    <!-- Explanation displayed under the Enable switch. -->
    <key>MESSAGES_EXPLANATION</key>
    <string>U aplikaciji Poruke, za pristup ovim opcijama pritisni “Details” dok si u razgovoru.</string>
 
    <!-- Message displayed when a conflicting tweak is installed. -->
    <key>MESSAGES_NOT_AVAILABLE_EXPLANATION</key>
    <string>Ova opcija je nedostupna zato što se sukobi sa SelectiveReading.</string>

	<!-- Switch in Messages app to enable/disable typing notifications. -->
	<key>SEND_TYPING_NOTIFICATIONS</key>
	<string>Pošalji Obavijesti o Pisanju</string>

	<!-- Explanation of the typing switch. -->
	<key>DISABLE_TYPING_NOTIFICATIONS_EXPLANATION</key>
	<string>Dopusti ili zabrani primatelju da vidi kada pišete.</string>

	<!-- Switch in Messages app to enable/disable read receipt notifications. -->
	<key>SEND_READ_RECEIPTS</key>
	<string>Pošalji Potvrdu Čitanja</string>

	<!-- Explanation of the read receipt switch. -->
	<key>DISABLE_READ_RECEIPTS_EXPLANATION</key>
	<string>Dopusti ili zabrani primatelju da vidi kada ste pročitali njihovu poruku.</string>

	<!-- Heading for the list of people. -->
	<key>PEOPLE</key>
	<string>Osobe</string>

	<!-- Button that allows another person to be added to the list. -->
	<key>ADD_PERSON</key>
	<string>Dodaj novo…</string>

	<!-- Button that allows a person to be removed. -->
	<key>REMOVE</key>
	<string>Ukloni</string>
</dict>
</plist>
