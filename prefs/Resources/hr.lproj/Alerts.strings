<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>Obavijesti</string>

	<!-- Header for the animation type segmented control. -->
	<key>ANIMATION_TYPE</key>
	<string>Animacije</string>

	<!-- Buttons shown in the segmented control for the animation type. -->
	<key>ANIMATION_SLIDE</key>
	<string>Klizni</string>
	<key>ANIMATION_FADE</key>
	<string>Izblijeđenje</string>

	<!-- Header for the display duration selection. -->
	<key>DISPLAY_DURATION</key>
	<string>Duljina Prikaza</string>

	<!-- The label showing how many seconds the user has specified. -->
	<key>ONE_SECOND</key>
	<string>1 Sekunda</string>
	<key>N_SECONDS</key>
	<string>%i Sekunde</string>

	<!-- The Typing and Read Receipt headings. -->
	<key>TYPING_HEADER</key>
	<string>Pisanje</string>
	<key>READ_HEADER</key>
	<string>Potvrda Čitanja</string>

	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Ugašeno</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Samo Ikona</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Statusna Traka</string>

	<!-- Button for choosing how long the alert shows for. -->
	<key>SHOW_FOR</key>
	<string>Prikaži Za</string>

	<!-- Options for "Show For". -->
	<key>SHOW_FOR_SPECIFIED</key>
	<string>Moju Određenu Duljinu</string>
	<key>SHOW_FOR_TYPING</key>
	<string>Duljinu Pisanja</string>

	<!-- Explanation shown under the "Show For" list. -->
	<key>SHOW_FOR_EXPLANATION</key>
	<string>Da bi se pokazala obavijest za duljinu određenu na prethodnoj stranici, odaberite "Moju Određenu Duljinu". Da bi se pokazala obavijest sve dok pošiljatelj piše poruku, izaberite "Duljinu Pisanja".</string>

	<!-- Switch to allow or disallow alerts showing in Messages. -->
	<key>HIDE_IN_MESSAGES</key>
	<string>Sakrijte u Porukama</string>

	<!-- Buttons to test the alerts. -->
	<key>TEST_TYPING</key>
	<string>Testiraj Obavijest Pisanja</string>
	<key>TEST_READ_RECEIPT</key>
	<string>Testiraj Obavijest Potvrde Čitanja</string>
</dict>
</plist>
