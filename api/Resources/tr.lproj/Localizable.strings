<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Strings used in status bar notifications to describe the type of message. -->
	<key>TYPING</key>
	<string>Yazıyor:</string>
	<key>READ</key>
	<string>Okundu:</string>
	<key>SENDING_FILE</key>
	<string>Dosya Göndeliyor:</string>

	<!-- Strings used in status bar notifications to naturally describe the notification. -->
	<key>TYPING_NATURAL</key>
	<string>%@ yazıyor</string>
	<key>READ_NATURAL</key>
	<string>%@ tarafından okundu</string>
	<key>SENDING_FILE_NATURAL</key>
	<string>Dosya %@ gönderildi </string>
</dict>
</plist>
